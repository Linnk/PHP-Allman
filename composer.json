{
    "name": "fabpot/php-cs-fixer",
    "type": "application",
    "description": "A script to automatically fix Symfony Coding Standard",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        }
    ],
    "require": {
        "php": ">=5.3.6",
        "symfony/console": "~2.1",
        "symfony/filesystem": "~2.1",
        "symfony/finder": "~2.1",
        "symfony/stopwatch": "~2.5",
        "sebastian/diff": "1.1.*"
    },
    "autoload": {
        "psr-0": { "Symfony\\CS": "." }
    },
    "bin": ["php-cs-fixer"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
